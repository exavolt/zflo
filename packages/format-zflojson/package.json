{
  "name": "@zflo/format-zflojson",
  "version": "0.1.0",
  "description": "ZFlo JSON format support for ZFlo",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest --run --passWithNoTests",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@zflo/core": "workspace:*",
    "@zflo/api-format": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "zflo",
    "json",
    "format",
    "parser",
    "flowchart"
  ],
  "author": "ZFlo Team",
  "license": "MIT"
}
